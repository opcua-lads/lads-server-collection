            <Reference ReferenceType="GeneratesEvent">i=2133</Reference>
            <Reference ReferenceType="HasProperty">ns=1;i=6016</Reference>
            <Reference ReferenceType="HasComponent">ns=1;i=5003</Reference>
        </References>
    </UAObjectType>
