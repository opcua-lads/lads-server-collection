{
  "$defs": {
    "measurementDocumentItems": {
      "properties": {
        "analyte name": {
          "$asm.pattern": "value datum",
          "$asm.property-class": "http://purl.allotrope.org/ontologies/result#AFR_0001604",
          "$asm.type": "http://www.w3.org/2001/XMLSchema#string",
          "$ref": "http://purl.allotrope.org/json-schemas/adm/core/REC/2025/03/core.schema#/$defs/tStringValue"
        },
        "data cube": {
          "$asm.pattern": "datacube",
          "$asm.property-class": "http://purl.org/linked-data/cube#DataSet",
          "allOf": [
            {
              "$ref": "http://purl.allotrope.org/json-schemas/adm/core/REC/2025/03/cube.schema#/$defs/tDatacube"
            },
            {
              "properties": {
                "cube-structure": {
                  "properties": {
                    "dimensions": {
                      "maxItems": 1,
                      "minItems": 1,
                      "prefixItems": [
                        {
                          "properties": {
                            "@componentDatatype": {
                              "const": "double"
                            },
                            "concept": {
                              "const": "elapsed time"
                            },
                            "unit": {
                              "const": "s"
                            }
                          }
                        }
                      ]
                    },
                    "measures": {
                      "contains": {
                        "oneOf": [
                          {
                            "properties": {}
                          }
                        ]
                      },
                      "maxItems": 1,
                      "minItems": 1
                    }
                  }
                }
              }
            }
          ],
          "type": "object"
        },
        "probe identifier": {
          "$asm.pattern": "value datum",
          "$asm.property-class": "http://purl.allotrope.org/ontologies/result#AFR_0002300",
          "$asm.type": "http://www.w3.org/2001/XMLSchema#string",
          "$ref": "http://purl.allotrope.org/json-schemas/adm/core/REC/2025/03/core.schema#/$defs/tStringValue"
        },
        "probe type": {
          "$asm.pattern": "value datum",
          "$asm.property-class": "http://purl.allotrope.org/ontologies/result#AFR_0002299",
          "$asm.type": "http://www.w3.org/2001/XMLSchema#string",
          "$ref": "http://purl.allotrope.org/json-schemas/adm/core/REC/2025/03/core.schema#/$defs/tStringValue"
        }
      },
      "required": [
        "probe identifier",
        "analyte name",
        "data cube",
        "probe type"
      ]
    }
  },
  "$id": "http://purl.allotrope.org/json-schemas/adm/automated-reactors/REC/2025/03/concentration-profile-detector.schema",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}